{
  "name": "F1 TV Playback Controls",
  "version": "0.1.8",
  "manifest_version": 2,
  "description": "Modern playback controls and Google Cast support for F1 TV",
  "homepage_url": "http://outlandnish.com",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [ "https://f1tv.formula1.com/*" ],
      "js": [ "src/inject.js" ],
      "css": [ "src/playback.css" ]
    }
  ],
  "permissions": [ "https://f1tv.formula1.com/*" ],
  "web_accessible_resources": [
    "src/cast.js",
    "src/controls.js",
    "src/startup.js",
    "src/playback.css"
  ],
  "externally_connectable": {
    "matches": ["https://f1tv.formula1.com/*"]
  }
}