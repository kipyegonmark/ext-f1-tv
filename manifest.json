{
  "name": "F1 TV Playback Controls",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Modern playback controls for F1 TV",
  "homepage_url": "http://outlandnish.com",
  "content_scripts": [
    {
      "matches": [
        "https://f1tv.formula1.com/*/current-season/*",
        "https://f1tv.formula1.com/*/archive/*"
      ],
      "js": [
        "src/inject.js"
      ]
    }
  ],
  "permissions": [    
    "https://f1tv.formula1.com/*"
  ],
  "web_accessible_resources": [
    "src/cast.js",
    "src/controls.js",
    "src/startup.js"
  ],
  "externally_connectable": {
    "matches": ["https://f1tv.formula1.com/*"]
  }
}